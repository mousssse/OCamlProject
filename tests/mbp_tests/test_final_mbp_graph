/* This is a dot format graph. */
digraph finite_state_machine {
	rankdir=LR;
    11 9 10 6 8 5 2 0 1 3 ;
    11 -> 5 [label = "1/1"];
    9 -> 2 [label = "1/1"];
    10 -> 2 [label = "0/1"];
    10 -> 3 [label = "1/1"];
    6 -> 1 [label = "1/1"];
    6 -> 2 [label = "0/1"];
    8 -> 0 [label = "1/1"];
    8 -> 3 [label = "0/1"];
}