/* This is a dot format graph. */
digraph finite_state_machine {
	rankdir=LR;
    11 10 9 8 6 4 0 1 2 3 5 7 ;
    11 -> 5 [label = 1];
    10 -> 3 [label = 1];
    10 -> 2 [label = 1];
    9 -> 2 [label = 1];
    8 -> 3 [label = 1];
    8 -> 0 [label = 1];
    6 -> 2 [label = 1];
    6 -> 1 [label = 1];
    4 -> 6 [label = 1];
    4 -> 8 [label = 1];
    4 -> 9 [label = 1];
    4 -> 10 [label = 1];
    4 -> 11 [label = 1];
    0 -> 7 [label = 1];
    1 -> 7 [label = 1];
    2 -> 7 [label = 1];
    3 -> 7 [label = 1];
    5 -> 7 [label = 1];
}