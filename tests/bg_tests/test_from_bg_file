/* This is a dot format graph. */
digraph finite_state_machine {
	rankdir=LR;
    0 1 2 3 4 ;
    0 -> 1 [label = "30 (7)"];
    0 -> 2 [label = "20 (6)"];
    1 -> 2 [label = "25 (5)"];
    1 -> 3 [label = "10 (4)"];
    2 -> 3 [label = "20 (2)"];
    2 -> 4 [label = "25 (2)"];
    3 -> 4 [label = "20 (1)"];
}